        const response = await axios.post('/admin_login', this.form)\r\n                    \r\n\r\n                    if(response.status === 200){\r\n                        alert({'message':'Login Successfully'})\r\n                        console.log(response)\r\n                    }\r\n                    else if(response.status===404){\r\n                        console.log(response)\r\n                        this.errorMessage='404 error'\r\n                    }\r\n\r\n            }\r\n        }\r\n    }            \r\n</script>\r\n\r\n<style>\r\n.login-form {\r\n    width: 300px;\r\n    margin: 0 auto;\r\n    padding: 20px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 8px;\r\n  }\r\n\r\n.form-group{\r\n    margin-bottom:15px;\r\n}\r\n\r\ninput {\r\n    width: 100%;\r\n    padding: 8px;\r\n    margin: 5px 0;\r\n    border-radius: 4px;\r\n    border: 1px solid #ccc;\r\n  }\r\n\r\n  button {\r\n    width: 100%;\r\n    padding: 10px;\r\n    background-color: #4CAF50;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n  }\r\n\r\n  .error-message {\r\n    color: red;\r\n    font-size: 14px;\r\n    margin-top: 10px;\r\n  }\r\n</style>"],"mappings":"AAqBA,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAc;EACNC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,IAAI,EAAC;QACDC,KAAK,EAAC,EAAE;QACRC,QAAQ,EAAC;MACb,CAAC;MACDC,YAAY,EAAC;IAEjB;EACJ,CAAC;EACDC,OAAO,EAAC;IACJ,MAAMC,gBAAgBA,CAAA,EAAE;MAChB,MAAMC,QAAO,GAAI,MAAMR,KAAK,CAACS,IAAI,CAAC,cAAc,EAAE,IAAI,CAACP,IAAI;MAG3D,IAAGM,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAC;QACvBC,KAAK,CAAC;UAAC,SAAS,EAAC;QAAoB,CAAC;QACtCC,OAAO,CAACC,GAAG,CAACL,QAAQ;MACxB,OACK,IAAGA,QAAQ,CAACE,MAAM,KAAG,GAAG,EAAC;QAC1BE,OAAO,CAACC,GAAG,CAACL,QAAQ;QACpB,IAAI,CAACH,YAAY,GAAC,WAAU;MAChC;IAER;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}