n    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);\r\n    padding: 30px;\r\n    text-align: center;\r\n    width: 250px;\r\n    transition: transform 0.3s ease, box-shadow 0.3s ease;\r\n  }\r\n  \r\n  .login-card:hover {\r\n    transform: translateY(-10px);\r\n    box-shadow: 0 6px 15px rgba(0, 0, 0, 0.3);\r\n  }\r\n  \r\n  .icon {\r\n    font-size: 4rem;\r\n    color: #ff7f50;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .login-btn {\r\n    background-color: #ff7f50;\r\n    color: white;\r\n    font-size: 1.1rem;\r\n    padding: 12px 25px;\r\n    border: none;\r\n    border-radius: 8px;\r\n    cursor: pointer;\r\n    text-transform: uppercase;\r\n    font-weight: bold;\r\n    transition: background-color 0.3s ease, transform 0.3s ease;\r\n    margin-top: 20px;\r\n  }\r\n  \r\n  .login-btn:hover {\r\n    background-color: #ff6f36;\r\n    transform: scale(1.05);\r\n  }\r\n  \r\n  .login-btn:focus {\r\n    outline: none;\r\n  }\r\n  \r\n  .login-btn router-link {\r\n    text-decoration: none;\r\n    color: white;\r\n  }\r\n  \r\n  /* Responsive Design */\r\n  @media (max-width: 768px) {\r\n    .hero-text h1 {\r\n      font-size: 2.5rem;\r\n    }\r\n  \r\n    .hero-text p {\r\n      font-size: 1.2rem;\r\n    }\r\n  \r\n    .login-options {\r\n      flex-direction: column;\r\n      align-items: center;\r\n    }\r\n  \r\n    .login-card {\r\n      width: 80%;\r\n      margin-bottom: 20px;\r\n    }\r\n  }\r\n  </style>\r\n  "],"mappings":"AAkEE,eAAe;EACbA,IAAI,EAAE;AACR","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}