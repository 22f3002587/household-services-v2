{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      user_data: {\n        fullname: 'John Doe',\n        email: 'john.doe@example.com'\n        // Add other user properties as needed\n      },\n      serviceCategories: ['Cleaning', 'Home Decoration', 'Health & Wellness', 'Saloon', 'Electrician'],\n      services: [{\n        service_id: 1,\n        service_category: 'Cleaning',\n        service_name: 'House Cleaning',\n        description: 'Complete home cleaning',\n        base_price: 500\n      }, {\n        service_id: 2,\n        service_category: 'Home Decoration',\n        service_name: 'Living Room Decor',\n        description: 'Redecorate living room',\n        base_price: 2000\n      }, {\n        service_id: 3,\n        service_category: 'Health & Wellness',\n        service_name: 'Yoga Class',\n        description: 'Yoga sessions for health',\n        base_price: 150\n      }, {\n        service_id: 4,\n        service_category: 'Saloon',\n        service_name: 'Haircut',\n        description: 'Professional hair cutting service',\n        base_price: 300\n      }, {\n        service_id: 5,\n        service_category: 'Electrician',\n        service_name: 'Wiring',\n        description: 'Electrical wiring repair',\n        base_price: 800\n      }],\n      service_requests: [{\n        request_id: 1,\n        service_name: 'House Cleaning',\n        status: 'Requested'\n      }, {\n        request_id: 2,\n        service_name: 'Living Room Decor',\n        status: 'Closed by customer'\n      }, {\n        request_id: 3,\n        service_name: 'Yoga Class',\n        status: 'Professional Accepted'\n      }]\n    };\n  },\n  methods: {\n    filterServices(category) {\n      return this.services.filter(service => service.service_category === category);\n    },\n    handleAction(request, action) {\n      if (action === 'close') {\n        alert(`Closing request ID: ${request.request_id}`);\n      } else if (action === 'delete') {\n        alert(`Deleting request ID: ${request.request_id}`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","user_data","fullname","email","serviceCategories","services","service_id","service_category","service_name","description","base_price","service_requests","request_id","status","methods","filterServices","category","filter","service","handleAction","request","action","alert"],"sources":["/home/shrisiitm/MAD 2 Project old/MAD 2 Project/frontend/src/components/CustomerHome.vue"],"sourcesContent":["<template>\n    <div class=\"customer-dashboard\">\n      <!-- Navbar -->\n      <div class=\"navbar\">\n        <h1>Welcome to Customer Dashboard</h1>\n        <div class=\"nav-links\">\n          <a href=\"#\">Home</a>\n          <a href=\"/customer_search\">Search</a>\n          <a href=\"/logout_user\">Logout</a>\n        </div>\n      </div>\n  \n      <!-- Welcome Section -->\n      <section class=\"welcome-section\">\n        <h2>Hello, {{ user_data.fullname }}</h2>\n      </section>\n  \n      <!-- Profile Section -->\n      <section class=\"profile-section\">\n        <button class=\"prof-button\">View Profile</button>\n        <div class=\"dropdown-profile-content\">\n          <img src=\"/static/image.png\" alt=\"user-image\" />\n          <h3>Hello</h3>\n          <p>Your Email: {{ user_data.email }}</p>\n          <p>Experience: 3 years</p>\n          <p>Specialisation: Cleaning</p>\n          <p>Admin Approval: Pending</p>\n        </div>\n      </section>\n  \n      <!-- Services Section -->\n      <section class=\"services-section\">\n        <h3>Looking for which service category?</h3>\n        <div v-for=\"(category, index) in serviceCategories\" :key=\"index\" class=\"service-category\">\n          <button class=\"category-btn\">{{ category }}</button>\n          <div class=\"dropdown-content\">\n            <table v-if=\"filterServices(category).length\">\n              <thead>\n                <tr>\n                  <th>Service Name</th>\n                  <th>Description</th>\n                  <th>Price</th>\n                  <th>Action</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-for=\"service in filterServices(category)\" :key=\"service.service_id\">\n                  <td>{{ service.service_name }}</td>\n                  <td>{{ service.description }}</td>\n                  <td>Rs. {{ service.base_price }}</td>\n                  <td>\n                    <form action=\"/customer_home\" method=\"POST\">\n                      <input type=\"hidden\" name=\"form_id\" :value=\"category\" />\n                      <input type=\"hidden\" name=\"service_id\" :value=\"service.service_id\" />\n                      <input type=\"hidden\" name=\"customer_email\" :value=\"user_data.email\" />\n                      <button class=\"btn\" type=\"submit\">Book</button>\n                    </form>\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n      </section>\n  \n      <!-- Requested Services Section -->\n      <section class=\"requested-services-section\">\n        <p class=\"header\">Requested Services:</p>\n        <table v-if=\"service_requests.length\">\n          <thead>\n            <tr>\n              <th>Request ID</th>\n              <th>Service Name</th>\n              <th>Status</th>\n              <th>Action</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"request in service_requests\" :key=\"request.request_id\">\n              <td>{{ request.request_id }}</td>\n              <td>{{ request.service_name }}</td>\n              <td>{{ request.status }}</td>\n              <td>\n                <button class=\"action-btn\" @click=\"handleAction(request, 'close')\">Close</button>\n                <button class=\"action-btn\" @click=\"handleAction(request, 'delete')\">Delete</button>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n        <p v-if=\"service_requests.length === 0\">Your service request is empty</p>\n      </section>\n    </div>\n  </template>\n  \n  <script>\n  export default {\n    data() {\n      return {\n        user_data: {\n          fullname: 'John Doe',\n          email: 'john.doe@example.com',\n          // Add other user properties as needed\n        },\n        serviceCategories: [\n          'Cleaning', 'Home Decoration', 'Health & Wellness', 'Saloon', 'Electrician'\n        ],\n        services: [\n          { service_id: 1, service_category: 'Cleaning', service_name: 'House Cleaning', description: 'Complete home cleaning', base_price: 500 },\n          { service_id: 2, service_category: 'Home Decoration', service_name: 'Living Room Decor', description: 'Redecorate living room', base_price: 2000 },\n          { service_id: 3, service_category: 'Health & Wellness', service_name: 'Yoga Class', description: 'Yoga sessions for health', base_price: 150 },\n          { service_id: 4, service_category: 'Saloon', service_name: 'Haircut', description: 'Professional hair cutting service', base_price: 300 },\n          { service_id: 5, service_category: 'Electrician', service_name: 'Wiring', description: 'Electrical wiring repair', base_price: 800 }\n        ],\n        service_requests: [\n          { request_id: 1, service_name: 'House Cleaning', status: 'Requested' },\n          { request_id: 2, service_name: 'Living Room Decor', status: 'Closed by customer' },\n          { request_id: 3, service_name: 'Yoga Class', status: 'Professional Accepted' }\n        ]\n      };\n    },\n    methods: {\n      filterServices(category) {\n        return this.services.filter(service => service.service_category === category);\n      },\n      handleAction(request, action) {\n        if (action === 'close') {\n          alert(`Closing request ID: ${request.request_id}`);\n        } else if (action === 'delete') {\n          alert(`Deleting request ID: ${request.request_id}`);\n        }\n      }\n    }\n  };\n  </script>\n  \n  <style scoped>\n  .customer-dashboard {\n    font-family: Arial, sans-serif;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding: 20px;\n  }\n  \n  .navbar {\n    width: 100%;\n    background-color: black;\n    color: white;\n    padding: 15px;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n  }\n  \n  .nav-links a {\n    color: azure;\n    margin-left: 10px;\n    text-decoration: none;\n    font-size: 16px;\n  }\n  \n  .welcome-section,\n  .profile-section,\n  .services-section,\n  .requested-services-section {\n    width: 100%;\n    margin-bottom: 30px;\n  }\n  \n  h2 {\n    font-size: 2rem;\n    color: #333;\n    text-align: center;\n  }\n  \n  h3 {\n    font-size: 1.5rem;\n    color: #555;\n  }\n  \n  .category-btn {\n    background-color: #1ce7e7;\n    border-radius: 5px;\n    padding: 10px;\n    cursor: pointer;\n    width: 100%;\n    text-align: left;\n    font-size: 1rem;\n    margin-top: 10px;\n  }\n  \n  .dropdown-content {\n    display: none;\n    position: absolute;\n    background-color: white;\n    width: 100%;\n    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);\n    padding: 10px;\n    top: 100%;\n    left: 0;\n  }\n  \n  .service-category:hover .dropdown-content {\n    display: block;\n  }\n  \n  .table,\n  button {\n    width: 100%;\n    margin-top: 10px;\n  }\n  \n  th,\n  td {\n    padding: 8px;\n    text-align: left;\n    border: 1px solid #ddd;\n  }\n  \n  .btn {\n    background-color: #5fb84f;\n    color: white;\n    border: none;\n    padding: 10px 20px;\n    cursor: pointer;\n    border-radius: 5px;\n  }\n  \n  .btn:hover {\n    background-color: #4fa94f;\n  }\n  \n  .action-btn {\n    background-color: #ff4d4d;\n    color: white;\n    border: none;\n    padding: 10px;\n    cursor: pointer;\n    border-radius: 5px;\n    margin: 5px;\n  }\n  \n  .action-btn:hover {\n    background-color: #ff1a1a;\n  }\n  </style>\n  "],"mappings":";;AA+FE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;QACTC,QAAQ,EAAE,UAAU;QACpBC,KAAK,EAAE;QACP;MACF,CAAC;MACDC,iBAAiB,EAAE,CACjB,UAAU,EAAE,iBAAiB,EAAE,mBAAmB,EAAE,QAAQ,EAAE,aAAY,CAC3E;MACDC,QAAQ,EAAE,CACR;QAAEC,UAAU,EAAE,CAAC;QAAEC,gBAAgB,EAAE,UAAU;QAAEC,YAAY,EAAE,gBAAgB;QAAEC,WAAW,EAAE,wBAAwB;QAAEC,UAAU,EAAE;MAAI,CAAC,EACvI;QAAEJ,UAAU,EAAE,CAAC;QAAEC,gBAAgB,EAAE,iBAAiB;QAAEC,YAAY,EAAE,mBAAmB;QAAEC,WAAW,EAAE,wBAAwB;QAAEC,UAAU,EAAE;MAAK,CAAC,EAClJ;QAAEJ,UAAU,EAAE,CAAC;QAAEC,gBAAgB,EAAE,mBAAmB;QAAEC,YAAY,EAAE,YAAY;QAAEC,WAAW,EAAE,0BAA0B;QAAEC,UAAU,EAAE;MAAI,CAAC,EAC9I;QAAEJ,UAAU,EAAE,CAAC;QAAEC,gBAAgB,EAAE,QAAQ;QAAEC,YAAY,EAAE,SAAS;QAAEC,WAAW,EAAE,mCAAmC;QAAEC,UAAU,EAAE;MAAI,CAAC,EACzI;QAAEJ,UAAU,EAAE,CAAC;QAAEC,gBAAgB,EAAE,aAAa;QAAEC,YAAY,EAAE,QAAQ;QAAEC,WAAW,EAAE,0BAA0B;QAAEC,UAAU,EAAE;MAAI,EACpI;MACDC,gBAAgB,EAAE,CAChB;QAAEC,UAAU,EAAE,CAAC;QAAEJ,YAAY,EAAE,gBAAgB;QAAEK,MAAM,EAAE;MAAY,CAAC,EACtE;QAAED,UAAU,EAAE,CAAC;QAAEJ,YAAY,EAAE,mBAAmB;QAAEK,MAAM,EAAE;MAAqB,CAAC,EAClF;QAAED,UAAU,EAAE,CAAC;QAAEJ,YAAY,EAAE,YAAY;QAAEK,MAAM,EAAE;MAAwB;IAEjF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACC,QAAQ,EAAE;MACvB,OAAO,IAAI,CAACX,QAAQ,CAACY,MAAM,CAACC,OAAM,IAAKA,OAAO,CAACX,gBAAe,KAAMS,QAAQ,CAAC;IAC/E,CAAC;IACDG,YAAYA,CAACC,OAAO,EAAEC,MAAM,EAAE;MAC5B,IAAIA,MAAK,KAAM,OAAO,EAAE;QACtBC,KAAK,CAAC,uBAAuBF,OAAO,CAACR,UAAU,EAAE,CAAC;MACpD,OAAO,IAAIS,MAAK,KAAM,QAAQ,EAAE;QAC9BC,KAAK,CAAC,wBAAwBF,OAAO,CAACR,UAAU,EAAE,CAAC;MACrD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}